{
  "manifest_version": 3,
  "name": "Webnote",
  "version": "1.0.1",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}